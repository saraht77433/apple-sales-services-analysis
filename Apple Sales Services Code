    "# Step 3: Bar Plot - Total Sales by Product"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "de9c66eb-4354-4e22-8fb0-235a572ea8eb",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 800x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Create a DataFrame for total sales by product\n",
    "product_sales = {\n",
    "    'Product': ['iPhone', 'iPad', 'Mac', 'Wearables', 'Services'],\n",
    "    'Total': [\n",
    "        apple_sales['iPhone Sales (in million units)'].sum(),\n",
    "        apple_sales['iPad Sales (in million units)'].sum(),\n",
    "        apple_sales['Mac Sales (in million units)'].sum(),\n",
    "        apple_sales['Wearables (in million units)'].sum(),\n",
    "        apple_sales['Services Revenue (in billion $)'].sum()\n",
    "    ]\n",
    "}\n",
    "\n",
    "product_df = pd.DataFrame(product_sales)\n",
    "\n",
    "# Plot\n",
    "plt.figure(figsize=(8, 5))\n",
    "sns.barplot(data=product_df, x='Product', y='Total')\n",
    "plt.title(\"Total Sales by Product Type\")\n",
    "plt.ylabel(\"Units Sold (Millions) / Revenue (Billions)\")\n",
    "plt.xlabel(\"\")\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
